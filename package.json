{
  "name": "@tripair/react-pdf",
  "version": "1.0.0",
  "description": "Display PDFs in your React app as easily as if they were images.",
  "main": "dist/umd/entry.js",
  "module": "dist/esm/entry.js",
  "source": "src/entry.js",
  "scripts": {
    "build": "npm run build-js-all && npm run copy-styles",
    "build-js-all": "npm run build-js-esm && npm run build-js-umd",
    "build-js-esm": "cross-env BABEL_ENV=production-esm babel src -d dist/esm --ignore **/*.spec.js,**/*.spec.jsx",
    "build-js-umd": "cross-env BABEL_ENV=production-umd babel src -d dist/umd --ignore **/*.spec.js,**/*.spec.jsx",
    "clean": "rimraf dist",
    "copy-styles": "node ./copy-styles.js",
    "jest": "jest",
    "jest-coverage": "jest --coverage",
    "lint": "eslint sample/ src/ test/ --ext .jsx,.js",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run lint && npm run jest"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "**/src/**.{js,jsx}",
      "!**/src/entry.js",
      "!**/src/entry.*.js",
      "!**/src/pdf.worker.entry.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "utils.js"
    ]
  },
  "keywords": [
    "pdf",
    "pdf-viewer",
    "react"
  ],
  "author": {
    "name": "Leon Sting"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "make-cancellable-promise": "^1.0.0",
    "make-event-props": "^1.1.0",
    "merge-class-names": "^1.1.1",
    "pdfjs-dist": "^2.4.456",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.35.0"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "files": [
    "README.md",
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leonsting/react-pdf.git"
  }
}
